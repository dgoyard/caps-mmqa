<pipeline>
    <docstring>
        Aggregate QA process results and plot QA scores
    </docstring>
    <processes>
        <standard name="plot_results" processing="False">
            <module>mmqa.fmri.plot_results.plot_results</module>
            <force name="output_directory" value="Undefined"/>
        </standard>
    </processes>
    <inputs>
        <input name="score_to_plot" dest="plot_results.selection"/>
        <input name="n_bins" dest="plot_results.n_bins" />
        <input name="group_list" dest="plot_results.groups"/>
        <input name="file_list" dest="plot_results.file_list"/>
    </inputs>
    <outputs>
        <output name="output_directory" src="plot_results.output_dir"/>
    </outputs>
    <positions>
        <position process="inputs" x="-26" y="199"/>
        <position process="plot_results" x="199" y="199"/>
        <position process="outputs" x="371" y="300"/>
    </positions>
    <scale factor="1.0"/>
</pipeline>
